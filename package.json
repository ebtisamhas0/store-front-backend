{
  "name": "store-front",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn prebuild && npx tsc",
    "create-test-db": "db-migrate --config database_create.json db:create shopping",
    "dev-migrations": "db-migrate up -e dev",
    "prebuild": "rimraf dist",
    "reset-test-db": "db-migrate reset -e test",
    "start": "yarn build && node ./build/index.js",
    "reset-dev-db": "db-migrate reset -e dev",
    "start-dev": "nodemon --watch src src/index.ts -r ts-node/register --inspect",
    "test": "db-migrate up -e test && yarn build && NODE_ENV=test jasmine && yarn reset-test-db"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.13",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "migrate": "^1.8.0",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "postgres": "^3.3.2",
    "supertest": "^6.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/jasmine": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/pg": "^8.6.5",
