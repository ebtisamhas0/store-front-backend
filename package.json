    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "node-env": "^0.1.6",
    "nodemon": "^2.0.20",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
